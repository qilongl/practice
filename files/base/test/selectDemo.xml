<?xml version="1.0" encoding="UTF-8"?>
<Epia id="selectDemo" datasource="dataSource" name="查询样例" desc="查询标签用法">
    <Parameters>
        <test>
            <test/>
            <test2 default=""/>
        </test>
        <test2>
            <test/>
            <test2 default="123"/>
        </test2>
        <test3>
            <!--${系统属性}-->
            <name default="${fulldate}"/>
        </test3>
        <!--数组不支持给定对象默认值，（无法判断具体数组大小）（若数组参数设置非必填属性，前端不传递参数，则引用该参数的命令结果集返还空数组（不执行该命令））-->
        <test4 islist="true">
            <name default="测试"/>
        </test4>
        <test5>
            <!--expression：参数正则表达式   desc:正则表达式验证失败时异常提示信息-->
            <test expression="[0-1]" desc="参数test只接受0，1"/>
            <test2 default="123" expression="[0-1]" desc="参数test只接受0，1"/>
        </test5>
    </Parameters>
    <Actions>
        <!--标签必填属性：id-->
        <!--该结果集不会输出-->
        <select id="Retrieve" isreturn="true">
            <![CDATA[
            SELECT sysdate from dual 
            ]]>
        </select>
        

    </Actions>
</Epia>